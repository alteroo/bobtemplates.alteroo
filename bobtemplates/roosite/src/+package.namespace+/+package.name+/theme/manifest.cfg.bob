[theme]
title = {{{ package.dottedname }}}
description = A diazo theme for {{{ package.namespace }}}
development-css = /++theme++{{{ package.dottedname }}}/styles.less
production-css = /++theme++{{{ package.dottedname }}}/styles.css

[theme:overrides]
directory = template-overrides

[theme:parameters]
portal_url = portal_state/portal_url
ajax_load = python: 'ajax_load' in request.form
anon_user = portal_state/anonymous
